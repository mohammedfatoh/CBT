@model Eximination
@if(Model.Result.ToLower().Equals("healthy"))
{
        <h1>Advices</h1>
}
else if (Model.Result.ToLower().Equals("firstCancer"))
{
    
    <h1>Tratement@(Model.MCH)</h1>

<div class="center2">
   
    <table class="table text-light">
      <thead>
        <tr>
          <th scope="col">TreatmentName</th>
          <th scope="col">Quantity</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          @foreach (var item in ViewBag.Treatments){
          <td>@item</td>
          <td>@item</td>

          }
        </tr>
      </tbody>
    </table>
       
</div>

}
else if (Model.Result.ToLower().Equals("SecondCancer"))
{
    
    <h1>Tratement@(Model.MCH)</h1>

<div class="center2">
   
    <table class="table text-light">
      <thead>
        <tr>
           <th scope="col">#</th>
          <th scope="col">TreatmentName</th>
          <th scope="col">Quantity</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">1</th>
          <td>D</td>
          <td>3</td>
        </tr>
      </tbody>
    </table>
       
</div>
 

}

